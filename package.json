    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "jest": "^21.2.1",
    "jsdom": "^11.3.0",
    "jsom": "^1.0.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "should": "^13.1.2",
    "style-loader": "^0.19.0",
